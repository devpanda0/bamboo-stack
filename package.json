{
  "name": "bamboo-stack",
  "description": "TanStack Start starter with tRPC, ArkType Prisma ORM, better-auth and TailwindCSS",
  "author": "DevPanda",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "better-auth",
    "biome",
    "arktype",
    "postgres",
    "postgresql",
    "prisma",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vite build && tsc --noEmit",
    "check": "npx @biomejs/biome check --write .",
    "db:generate": "npxprisma generate",
    "db:migrate": "npxprisma migrate",
    "db:push": "npxprisma push",
    "db:seed": "npx tsx src/db/seed.ts ",
    "db:studio": "npxprisma studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "postinstall": "npx prisma generate",
    "start": "node --env-file=.env .output/server/index.mjs",
    "typecheck": "npx tsc --noEmit "
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.12.0",
    "@prisma/client": "^6.12.0",
    "@tanstack/react-form": "^1.15.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.130.2",
    "@tanstack/react-router-with-query": "^1.130.2",
    "@tanstack/react-start": "^1.130.2",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@trpc/tanstack-react-query": "^11.4.3",
    "@unpic/react": "^1.0.1",
    "arktype": "^2.1.20",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "prisma": "^6.12.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
